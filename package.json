{
  "name": "hbs-kit",
  "version": "1.2.0",
  "description": "A portable collection of handlebars helper functions for use in node and the browser.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "tap -R list src/test.js src/test-util.js",
    "test-dev": "tap -R classic --no-coverage src/test.js",
    "test-util": "tap -R classic --no-coverage src/test-util.js",
    "docs": "node docs/index.js",
    "lint": "eslint ./ --fix",
    "build": "rollup -c",
    "clean": "rm -rf ./dist ./util.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twipped/handlebar-kit"
  },
  "keywords": [
    "handlebars",
    "helpers"
  ],
  "engines": {
    "node": ">=8.14.0"
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.7.7",
    "@babel/preset-env": "~7.7.7",
    "@rollup/plugin-node-resolve": "~6.1.0",
    "babel-eslint": "~10.0.3",
    "date-fns": "~2.8.1",
    "dox": "~0.9.0",
    "eslint": "~6.8.0",
    "eslint-config-twipped": "~3.3.0",
    "eslint-plugin-node": "~11.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "glob": "~7.1.6",
    "handlebars": "~4.5.3",
    "highlight.js": "~9.17.1",
    "markdown-it": "~10.0.0",
    "markdown-it-anchor": "~5.2.5",
    "markdown-it-highlightjs": "~3.3.1",
    "rollup": "~1.27.14",
    "rollup-plugin-babel": "~4.3.3",
    "rollup-plugin-banner": "~0.2.1",
    "rollup-plugin-terser": "~5.1.3",
    "tap": "~14.10.5"
  },
  "peerDependencies": {
    "handlebars": ">= 3.0.0"
  },
  "files": [
    "dist",
    "docs/index.html",
    "CHANGELOG.md"
  ],
  "dependencies": {}
}
